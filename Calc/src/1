
                    public class Factories {
                        public static  void EXP (ServiceFactory fact,Stack<Double> stack){
                            ICommand s = fact.getIComand();
                            double a = stack.pop();
                            Math.exp(a);
                            stack.push(a);

                        }

                        public static  void LOG (ServiceFactory fact,Stack<Double> stack){
                            ICommand s = fact.getIComand();
                            double a = stack.pop();
                            Math.log(a);
                            stack.push(a);

                        }